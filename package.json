{
  "name": "react-webpack-babel-express-router",
  "version": "0.0.1",
  "description": "React, Node, Express, ReactRoute, Webpack and Babel",
  "main": "src/server.js",
  "scripts": {
    "dev-server": "nodemon src/server.js --exec babel-node",
    "dev-watch": "webpack --watch",
    "dev": "concurrently -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-watch\"",
    "start": "node dist/server.bundle.js",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git"
  },
  "author": "Jason Lin",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "webpack-dashboard": "^1.0.0-2",
    "webpack-dev-server": "^2.4.1",
    "webpack-node-externals": "^1.2.0"
  }
}
